{
  "extends": "astro/tsconfigs/base",
  "types": [
    "@astrojs/image/client"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "paths": {
      "@com/*": [
        "src/components/*"
      ],
      "@lay/*": [
        "src/layouts/*"
      ]
    },
    "jsxImportSource": "preact"
  }
}